file=$HOME/.aws/$1.credentials 
if [ ! -f $file ]; then
  echo "$file does not exist.  Please create a template file for this account in the format of"
  echo "----FILE-BEGIN------"
  echo "AWSAccessKeyId=<Write your AWS access ID>"
  echo "AWSSecretKey=<Write your AWS secret key>"
  echo "----FILE-END-------"
else
  chmod 600 $file
  export AWS_CREDENTIAL_FILE=$file
fi
